{
    "name": "Backpacks",
    "description": "Inspirers' resource organizer.",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage", "activeTab", "scripting", "notifications"],
    "action": {
        "default_popup": "popup.html"
    }
}